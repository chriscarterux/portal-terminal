# Portal Terminal Configuration

# AI Provider API Keys (Optional - Local models work without these)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_claude_key_here
GOOGLE_API_KEY=your_gemini_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here
QWEN_API_KEY=your_qwen_key_here

# Local AI Model Configuration
LOCAL_AI_ENABLED=true
GPU_ACCELERATION=true
MODEL_CACHE_DIR=./models
MAX_MODEL_MEMORY=8192

# MCP Server Configuration
MCP_ENABLED=true
MCP_AUTO_DISCOVERY=true
MCP_HEALTH_CHECK_INTERVAL=30000

# GitHub Integration (Optional)
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# Context7 Documentation Service (Optional) 
CONTEXT7_API_KEY=your_context7_key_here

# Performance Settings
PERFORMANCE_MONITORING=true
AI_RESPONSE_TIMEOUT=5000
TERMINAL_BUFFER_SIZE=10000

# Development Settings
NODE_ENV=development
ELECTRON_IS_DEV=true
LOG_LEVEL=debug