# Portal Terminal - AI Provider Configuration
# Copy this file to .env and add your API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_BASE_URL=https://api.openai.com/v1 # Optional custom endpoint

# Anthropic Configuration  
ANTHROPIC_API_KEY=your_anthropic_api_key_here
# ANTHROPIC_BASE_URL=https://api.anthropic.com # Optional custom endpoint

# Google AI Configuration
GOOGLE_API_KEY=your_google_api_key_here

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Qwen Configuration (Alibaba Cloud)
QWEN_API_KEY=your_qwen_api_key_here

# MCP Server Configuration (Optional)
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
TAVILY_API_KEY=your_tavily_api_key_here
BRAVE_API_KEY=your_brave_search_api_key_here
CONTEXT7_API_KEY=your_context7_api_key_here

# PostgreSQL for MCP (Optional)
POSTGRES_CONNECTION_STRING=postgresql://user:pass@localhost:5432/dbname

# AI Model Preferences
PREFERRED_LOCAL_MODEL=gpt-oss-20b
PREFERRED_EXTERNAL_MODEL=claude-sonnet
MAX_DAILY_COST=5.00
MAX_RESPONSE_TIME=5000

# Performance Settings
ENABLE_LOCAL_AI=true
ENABLE_EXTERNAL_AI=true
ENABLE_AI_CACHING=true
PRELOAD_MODELS=false